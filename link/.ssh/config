Host *

# Forward ssh agent to the remote machine.
ForwardAgent yes

IgnoreUnknown UseKeychain
AddKeysToAgent yes
UseKeychain yes
IdentityFile ~/.ssh/id_rsa.pub

# Automatically add all common hosts to the host file as they are connected to.
StrictHostKeyChecking no

Host jumpbox.printify.com
  Hostname jumpbox.printify.com
  User martins-sipenko
  IdentityFile ~/.ssh/pfy-jumpbox
  IdentitiesOnly yes
